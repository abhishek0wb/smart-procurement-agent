    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "helmet": "^7.0.0",
    "http-status-codes": "^2.2.0",
    "imapflow": "^1.2.6",
    "jsonwebtoken": "^9.0.0",
    "mailparser": "^3.9.1",
    "morgan": "^1.10.0",
    "nodemailer": "^7.0.12",
    "openai": "^4.104.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
